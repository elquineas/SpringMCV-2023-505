<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
 <mapper namespace="com.elquineas.todolist.dao.TodoDao">
 
 	<insert id="insert">
 		INSERT INTO tbl_todolist (to_sdate, to_stime, to_content)
 		VALUE(#{to_sdate},#{to_stime},#{to_content})
 	</insert>
 	<update id="update"></update>
 
 </mapper> 